# Telegram Bot Token from BotFather
TELEGRAM_BOT_TOKEN=

# Admin user's Telegram ID
ADMIN_USER_ID=

# Redis URL (e.g., from Redis Labs, Upstash, or a local instance)
# redis://:password@hostname:port
REDIS_URL=

# Secret token for accessing the /admin panel
ADMIN_PANEL_TOKEN=

# (Optional) Set to "true" to force-enable autotrade for the admin on startup
ENABLE_AUTOTRADE=false

# (Optional) Encryption key for storing Binance API keys securely.
# Generate a strong random key. You can use: openssl rand -hex 32
BINANCE_ENCRYPTION_KEY=

# (Optional) Webhook for promotion events
PROMOTION_WEBHOOK_URL=
PROMOTION_WEBHOOK_SECRET=
ENABLE_PROMOTION_WEBHOOK=false
